{
  "extends": "airbnb-typescript-prettier",
  "rules": {
    "import/order": ["error", {
      "groups": ["builtin", "external", "parent", "sibling", "index"],
      "newlines-between": "always"
    }],
    "no-console": ["error", {
      "allow": ["info", "warn", "error"]
    }],
    "quotes": ["error", "double", {
      "avoidEscape": true
    }]
  }
}
